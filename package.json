{
  "name": "nikkiselev.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss}\"",
    "lint:fix": "eslint \"{src,test,examples}/**/*.{ts,tsx}\" --fix",
    "pre-commit": "lint-staged",
    "analyze": "env ANALYZE=true npm run build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.2.2",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4",
    "yarn-upgrade-all": "^0.5.4"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "next": "^10.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sitemap": "^7.0.0"
  }
}
